<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imag"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="@dimen/_17sdp"
        android:layout_marginBottom="@dimen/_481sdp"
        app:srcCompat="@drawable/noopath" />

    <ImageView
        android:id="@+id/imagfav"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="@dimen/_39sdp"
        android:layout_marginTop="@dimen/_38sdp"
        app:srcCompat="@drawable/ic_bookmark_filled" />

    <TextView
        android:id="@+id/textname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="@dimen/_271sdp"
        android:layout_marginEnd="@dimen/_40sdp"
        android:text="TextView" />

    <TextView
        android:id="@+id/textadress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="@dimen/_301sdp"
        android:layout_marginEnd="@dimen/_39sdp"
        android:text="TextView" />


    <RatingBar
        android:id="@+id/rating"
        style="?android:attr/ratingBarStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_328sdp"
        android:layout_marginEnd="@dimen/_39sdp"
        android:layout_alignParentEnd="true"
        android:numStars="5"
        android:stepSize="0.5" />

    <TextView
        android:id="@+id/textprice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="@dimen/_18sdp"
        android:layout_marginBottom="@dimen/_406sdp"
        android:text="TextView" />

    <TextView
        android:id="@+id/texdese"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="@dimen/_20sdp"
        android:layout_marginBottom="@dimen/_339sdp"
        android:text="TextView" />

    <ImageView
        android:id="@+id/imagloction"
        android:layout_width="@dimen/_66sdp"
        android:layout_height="@dimen/_61sdp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="@dimen/_18sdp"
        android:layout_marginBottom="@dimen/_337sdp"
        app:srcCompat="@drawable/location" />

</RelativeLayout>